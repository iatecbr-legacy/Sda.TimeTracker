@using Web.Models
@model ProjectsViewModel
@{
    ViewBag.Title = "Projects";
}
<h1>Team Project</h1>
<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null && Model.ProjectResult != null)
        {
            foreach (var project in Model.ProjectResult.value)
            {
                <tr>
                    <td>
                        <!-- Id -->
                        <a href="@project.url">@project.id</a>
                    </td>
                    <td>
                        <!-- Created Date -->
                        @project.name
                    </td>
                    <td>
                        @project.description
                    </td>
                    
                </tr>
            }
        }
    </tbody>
</table>
